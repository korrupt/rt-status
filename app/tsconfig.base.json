{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@app/auth-data-access": ["libs/api/auth/data-access/src/index.ts"],
      "@app/auth-feature": ["libs/api/auth/feature/src/index.ts"],
      "@app/auth-models": ["libs/api/auth/models/src/index.ts"],
      "@app/core": ["libs/api/core/src/index.ts"],
      "@app/device-data-access": ["libs/api/device/data-access/src/index.ts"],
      "@app/device-feature": ["libs/api/device/feature/src/index.ts"],
      "@app/device-models": ["libs/api/device/models/src/index.ts"],
      "@app/shared-models": ["libs/shared/models/src/index.ts"],
      "@app/typeorm-config": ["libs/api/typeorm/config/src/index.ts"],
      "@app/typeorm-feature": ["libs/api/typeorm/feature/src/index.ts"],
      "@app/user-data-access": ["libs/api/user/data-access/src/index.ts"],
      "@app/user-feature": ["libs/api/user/feature/src/index.ts"],
      "@app/user-models": ["libs/api/user/models/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
